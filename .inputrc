set mark-symlinked-directories on
set editing-mode vi

set keymap vi-insert
"\C-l": clear-screen
"\e[2~": quoted-insert
"\e[3~": vi-delete
"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": beginning-of-history
"\e[6~": end-of-history

set keymap vi-command
"\C-l": clear-screen
"\e[2~": quoted-insert
"\e[3~": vi-delete
"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": beginning-of-history
"\e[6~": end-of-history
"":  backward-char
"gg": beginning-of-history
"G": end-of-history


